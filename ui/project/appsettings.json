{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "App": "Data Source=Database/app.db"
  },
  "Configurations": {
    "DatasetsPath": "/workspace/datasets",
    "ConfigsPath": "/workspace/configs",
    "OutputsPath": "/workspace/outputs",
    "ModelsPath": "/workspace/models",
    "Models": {
      "SDXL": {
        "CheckpointPath": "{ModelsPath}/sdxl/sd_xl_base_1.0_0.9vae.safetensors"
      },
      "Chroma": {
        "DiffusersPath": "{ModelsPath}/chroma",
        "TransformerPath": "{ModelsPath}/chroma"
      },
      "Cosmos": {
        "DiffusersPath": "{ModelsPath}/cosmos/diffusers",
        "VaePath": "{ModelsPath}/cosmos/vae/cosmos_cv8x8x8_1.0.safetensors",
        "TextEncoderPath": "{ModelsPath}/cosmos/text_encoder/t5xxl_fp16.safetensors"
      },
      "Flux": {
        "DiffusersPath": "{ModelsPath}/flux/diffusers",
        "TransformerPath": "{ModelsPath}/flux/transformer/flux1-schnell.safetensors"
      },
      "LTX": {
        "DiffusersPath": "{ModelsPath}/ltx",
        "SingleFilePath": "{ModelsPath}/ltx"
      },
      "Lumina": {
        "TransformerPath": "{ModelsPath}/lumina",
        "VaePath": "{ModelsPath}/lumina",
        "LlmPath": "{ModelsPath}/lumina"
      },
      "Wan21": {
        "CkptPath": "{ModelsPath}/wan"
      },
      "Hunyuan": {
        "CkptPath": "{ModelsPath}/hunyuan/official",
        "TransformerPath": "{ModelsPath}/hunyuan/hunyuan_video_720_cfgdistill_fp8_e4m3fn.safetensors",
        "VaePath": "{ModelsPath}/hunyuan/hunyuan_video_vae_bf16.safetensors",
        "ClipPath": "{ModelsPath}/hunyuan/clip-vit-large-patch14",
        "LlmPath": "{ModelsPath}/hunyuan/llava-llama-3-8b-text-encoder-tokenizer"
      }
    },
    "UploadChunkFileSizeInMB": 100,
    "MaxFileSizeInMB": 1024,
    "NameDatasetDefault": "Default",
    "UsingDiffusionPipeFromFork": true,
    "DiffusionPipePath": "/workspace/diffusion-pipe",
    "EnvPath": "/workspace/diffusion-pipe/venv",
    "StartTrainingCommand": "source {EnvPath}/bin/activate && NCCL_P2P_DISABLE=\"1\" NCCL_IB_DISABLE=\"1\" deepspeed --num_gpus=1 {DiffusionPipePath}/train.py --deepspeed --config {TrainConfig}"
  },
  "AllowedHosts": "*"
}
